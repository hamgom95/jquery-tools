{
  "name": "@hamgom95/jquery-tools",
  "version": "0.1.0",
  "description": "jquery plugin",
  "main": "src/index.js",
  "keywords": [
    "jquery",
    "plugin"
  ],
  "repository": "hamgom95/jquery-tools",
  "scripts": {
    "test": "tap src/**/*.spec.js",
    "build:bundle": "rollup -c",
    "build:doc": "jsdoc src/*",
    "build:readme": "jsdoc2md --heading-depth=4 --template README.tpl.md src/* > README.md",
    "prepare": "npm run build:bundle && npm run build:doc && npm run build:readme",
    "prepublishOnly": "npm run build:readme"
  },
  "author": {
    "name": "Kevin Thomas",
    "email": "hamgom95@gmail.com"
  },
  "devDependencies": {
    "jquery": "^3.3.1",
    "jsdoc": "^3.5.5",
    "jsdoc-to-markdown": "^4.0.1",
    "jsdom": "^13.2.0",
    "rollup": "^1.6.0",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-terser": "^4.0.4",
    "tap": "^12.5.3"
  },
  "files": [
    "/src"
  ],
  "license": "ISC",
  "peerDependencies": {
    "jquery": "^3.3.1"
  }
}
